    "react-bootstrap-icons": ">=1.0.0",
    "react-dom": "^18.3.0",
    "react-router": ">=6.22.2",
    "typescript": "^5.7.2"
  },
  "devDependencies": {
    "@storybook/builder-vite": "^8.4.0",
    "@storybook/react-vite": "^8.4.0",
    "@types/prismjs": "^1.26.4",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "typescript": "^5.7.2",
    "vite": "^5.4.11"
